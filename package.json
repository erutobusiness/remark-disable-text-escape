{
	"name": "remark-disable-text-escape",
	"version": "1.0.0",
	"description": "A Remark plugin to prevent square brackets and asterisks from being escaped by remark-stringify",
	"type": "module",
	"exports": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/"
	],
	"scripts": {
		"build": "node node_modules/typescript/bin/tsc",
		"test": "node node_modules/vitest/vitest.mjs run",
		"lint": "node node_modules/@biomejs/biome/bin/biome check",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"remark",
		"remark-plugin",
		"markdown",
		"bracket",
		"asterisk",
		"escape",
		"unified"
	],
	"license": "MIT",
	"dependencies": {
		"unist-util-visit": "^5.0.0"
	},
	"peerDependencies": {
		"remark-stringify": "^11.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.0",
		"@types/mdast": "^4.0.0",
		"mdast-util-to-markdown": "^2.0.0",
		"remark": "^15.0.0",
		"remark-stringify": "^11.0.0",
		"typescript": "^5.0.0",
		"unified": "^11.0.0",
		"vitest": "^3.0.0"
	}
}
